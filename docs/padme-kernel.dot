    MM_Magnet2
    MMHV1_b1_ch2
    5
  }
  {rank=same
    MM_Magnet2
    MMHV1_b2_ch2
      MMHV1_crate
      6
  }


  subgraph cluster_drivers{
    graph[style=filled color=yellow];node[style=filled,fillcolor=white];
    rank=same
