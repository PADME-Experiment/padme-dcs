            MMHV1_b1_ch3
        }
    }
    subgraph cluster_MM_HV1board2{
      rank=same;
      label="board 2"
        graph[style=filled color=red]
        MMHV1_board2[label=<<b>board2</b><br/>Temperature<br/>SerialNumber<br/><b>Timestamp</b>>];
      MMHV1_b2_ch1
        MMHV1_b2_ch2
        MMHV1_b2_ch3
    }


  }
      }
    }
  }


